# Required
DEEPSEEK_API_KEY=your_deepseek_key

# Optional: DeepSeek compatible base URL/model
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_MODEL=deepseek-chat

# Optional: YouTube Data API (fallback to Invidious if omitted)
YOUTUBE_API_KEY=

# Optional: Google Programmable Search (fallback to Google News RSS if omitted)
GOOGLE_CSE_API_KEY=
GOOGLE_CSE_ID=

# Translation provider (default: mymemory)
TRANSLATE_PROVIDER=mymemory
MYMEMORY_EMAIL=

# Optional: Invidious instance
INVIDIOUS_BASE_URL=https://yewtu.be

# Networking
HTTP_TIMEOUT=18
MAX_CONCURRENCY=6
